  "private": true,
  "scripts": {
    "start": "yarn watch:src",
    "dev": "rimraf ./dist && concurrently yarn:watch:*",
    "dev-clean": "rimraf node_modules && yarn cache clean && yarn install && yarn dev",
    "watch:src": "cross-env NODE_ENV=development MODE_ENV=development MANIFEST_VERSION=3 webpack --watch --progress",
    "watch:dist": "mv3-hot-reload",
    "build": "cross-env NODE_ENV=development MODE_ENV=production MANIFEST_VERSION=3 webpack",
    "prebuild": "rimraf ./dist && yarn clear:webpack-cache",
    "build-all": "rimraf ./dist && run-s build:*",
    "build:chrome": "cross-env NODE_ENV=development MODE_ENV=production TARGET_BROWSER=chrome MANIFEST_VERSION=3 webpack && TARGET_BROWSER=chrome yarn zip",
    "build:firefox": "cross-env NODE_ENV=development MODE_ENV=production TARGET_BROWSER=firefox MANIFEST_VERSION=2 webpack && TARGET_BROWSER=firefox yarn zip",
    "build:safari": "cross-env NODE_ENV=development MODE_ENV=production TARGET_BROWSER=safari MANIFEST_VERSION=2 webpack && TARGET_BROWSER=safari yarn zip",
    "zip": "ts-node --project ./tsconfig-utility.json ./utility/buildZip.ts",
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage",
    "ts": "tsc --pretty",
    "lint": "eslint src",
    "lint:fix": "eslint --fix ./src",
    "clear:lint": "rimraf node_modules/.cache/eslint-loader",
    "clear:webpack-cache": "rimraf node_modules/.cache/webpack",
    "clean": "rimraf dist",
    "clean:install": "rimraf node_modules yarn.lock && yarn",
    "createTranslationFile": "ts-node --project ./tsconfig-utility.json ./utility/generateLanguageFiles.ts && ts-node --project ./tsconfig-utility.json ./utility/generateLanguageFiles.ts -e",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build -c .storybook -o storybook-deploy/public"
  },
